!!!
%html
  %head
    %title FancygridTestapp
    = stylesheet_link_tag :all
    = javascript_include_tag 'jquery', 'jquery-ui', 'fancygrid', 'application'
    = stylesheet_link_tag 'compiled/fancygrid', 'scaffold', 'jquery-ui'
    = csrf_meta_tag
  %body
  
    #tables
      %ul
        %li
          = link_to "List Roles", roles_path
        %li
          = link_to "List Users", users_path
        %li
          = link_to "List Companies", companies_path
    #content
      = yield
    
    #footer
      %pre
        = session.to_yaml.gsub("\n", "<br/>").html_safe
